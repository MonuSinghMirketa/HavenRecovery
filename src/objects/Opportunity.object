IF(INCLUDES(Facility_Interest__c, &quot;Indiana Center for Recovery&quot;), &quot;ICFR&quot;, 
IF(INCLUDES(Facility_Interest__c, &quot;Recovery Team&quot;), &quot;TRT&quot;,
