IF(INCLUDES(Level_of_Care__c, &quot;Residential&quot;), &quot;Residential&quot;,
IF(INCLUDES(Level_of_Care__c, &quot;PHP&quot;), &quot;PHP&quot;,
IF(INCLUDES(Level_of_Care__c, &quot;IOP&quot;), &quot;IOP&quot;,
IF(INCLUDES(Level_of_Care__c, &quot;OPT&quot;), &quot;OPT&quot;,
IF(INCLUDES(Level_of_Care__c, &quot;TBD&quot;), &quot;TBD By UR&quot;, &quot;0&quot;))))))</formula>
