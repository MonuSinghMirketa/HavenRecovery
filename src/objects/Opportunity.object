    <validationRules>
        <fullName>Admission_Alert_Sent_Validation</fullName>
        <active>true</active>
        <description>Only let us send an Admission if the relevant data is set.</description>
        <errorConditionFormula>Send_Admissions_Alert__c = true &amp;&amp;
(
ISBLANK(Scheduled_Admission_Date__c) ||
ISBLANK(Facility_Interest__c) ||
ISBLANK(TEXT(Payment_Method__c))||
ISBLANK(TEXT(LeadSource))||
ISBLANK( Transport_Info__c) ||
Scheduled_Admission_Date__c &lt; TODAY() ||
INCLUDES(Facility_Interest__c, &quot;TBD&quot;) ||  Facility_Confirmed__c = FALSE
  
)</errorConditionFormula>
        <errorMessage>You cannot send an Admission Alert without setting the Scheduled Admission Date, Facility of Interest, Payment Method and Transport Info</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Refer_Outs</fullName>
        <active>true</active>
        <errorConditionFormula>Send_to_Refer_Outs__c  = true &amp;&amp;
(
ISBLANK( Patient_Phone__c ) ||
ISBLANK( DOB__c ) ||
ISBLANK(TEXT(Payment_Method__c))
)</errorConditionFormula>
        <errorMessage>You must include Patient Phone, Payment Method, and DOB to send a Refer Out Email.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Referred_To_Required_Upon_Stage_Entry</fullName>
        <active>true</active>
        <description>If stage equals Referred Out (Reciprocating Partner) or Referred Out (Medicaid/Medicare) then Referred To is Required.</description>
        <errorConditionFormula>AND(
OR(
ISPICKVAL(StageName, &apos;Referred Out (Reciprocating Partner&apos;),
ISPICKVAL(StageName, &apos;Referred Out (Medicaid/Medicare)&apos;)
),
ISBLANK( Referred_To__c )
)</errorConditionFormula>
        <errorDisplayField>Referred_To__c</errorDisplayField>
        <errorMessage>If the stage contains Referred Out then the Field Referred To is Required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Date_for_CC</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(REGEX(Expiration_Date__c,&quot;^((0[1-9])|(1[0-2]))/[0-9]{4}$&quot;)) &amp;&amp; NOT(ISBLANK(Expiration_Date__c))</errorConditionFormula>
        <errorMessage>Expiration Date must be in MM/YYYY Format</errorMessage>
    </validationRules>
