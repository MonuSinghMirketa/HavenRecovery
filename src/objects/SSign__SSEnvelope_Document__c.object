        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(
  CONTAINS(SSign__Active_Signer_URL__c, SSign__SFSiteURL__c),
  IF(
    AND(NOT(ISBLANK(SSign__Active_Signer_URL__c)), SSign__Active__c),
    HYPERLINK(
      SSign__Active_Signer_URL__c,
      IMAGE(&quot;/img/icon/signature24.png&quot;,&quot;Continue Signing&quot;),
      &quot;_blank&quot;
    ),
    &apos;&apos;
  ),
  IF(
    AND(NOT(ISBLANK(SSign__Active_Signer_URL__c)), SSign__Active__c),
    HYPERLINK(
      SSign__SFSiteURL__c + SSign__Active_Signer_URL__c,
      IMAGE(&quot;/img/icon/signature24.png&quot;,&quot;Continue Signing&quot;),
      &quot;_blank&quot;
    ),
    &apos;&apos;
  )
)</formula>
